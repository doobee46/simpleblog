<div class="container_12">
    <div class="grid_12">
        <div class=" grid_3 alpha">
                    <div class=" aside">
                        <h1 class="logo"><%=link_to "Simple Blog", root_path%></h1>
                        <h3 class="logo_sub">My daily blog</h3>
                    </div>
                    <div class="aside_menu">
                        <div class="category">
                            <h1 class="cat_title"> Categories</h1>
                        </div>
                        <ul class="cat_list">
                            <% @category.each do |c| %>
                                <li>
                                    <%= link_to c.name %>
                                </li> 
                            <%end%> 
                        </ul> 
                       
                    </div>
                </div>
                
                <div class="grid_9 omega">

                    <div class="grid_9">
                        <div class="header">
                            
                            <div>
                               <%= render 'articles/search' %>
                            </div>
                          
                        </div>
                 
                        
                        
                        